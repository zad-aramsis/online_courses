<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_online_course" model="ir.sequence">
            <field name="name">Online Course Sequence</field>
            <field name="code">online.course.sequence</field>
            <field name="prefix">CRS/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_online_student_id_number" model="ir.sequence">
            <field name="name">Online Student ID Number Sequence</field>
            <field name="code">online.student.id.sequence</field>
            <field name="prefix">STU-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>